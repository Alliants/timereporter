(function(){
  var $entry = $('#time_entry_<%= @time_entry.id %>'),
      height = $entry.height() / 40,
      totalHours = parseFloat( $('.total-hours').text() );

  $entry.parent().removeClass('chosen');
  $entry.remove();

  timeEntries.totalHours = totalHours - height
  $('.total-hours').text( timeEntries.totalHours )

  if ($('.entry-form').length) {
    timeEntries.removeForm();
  }

})();
