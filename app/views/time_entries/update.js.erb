(function() {
  var $entryInner = $("#time_entry_<%= @time_entry.id %>").find('.inner'),
      $newEntryInner = $("<%=j render(@time_entry) %>").find('.inner'),
      $cover = $('.table-cover'),
      $form = $('.form-popout');

  if ($form.length) {
    $form.remove();
  }

  if ($cover.length) {
    $cover.remove();
  }

  $entryInner.replaceWith($newEntryInner);

})();
