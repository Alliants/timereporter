(function() {
  var $entry = $("#time_entry_<%= @time_entry.id %>"),
      $newEntry = $("<%=j render(@time_entry) %>");

  $entry.attr('class', $newEntry.attr('class'));

  if ($('.entry-form').length) {
    timeEntries.removeForm();
  }

  $entry.find('.inner').replaceWith($newEntry.find('.inner'));

})();
