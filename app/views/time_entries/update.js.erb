(function() {
  var $entry = $("#time_entry_<%= @time_entry.id %>"),
      $new_entry = $("<%=j render(@time_entry) %>"),
      $cover = $('.table-cover'),
      $form = $('.form-popout');

  if ($form.length){
    $form.remove();
  }

  if ($cover.length){
    $cover.remove();
  }

  $entry.replaceWith($new_entry);
  time_entries.set_resizeable($new_entry);
  time_entries.$entry = "";

})();